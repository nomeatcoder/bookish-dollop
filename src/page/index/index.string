{{#list}}
<h1 class="floor-title">{{name}}</h1>
    <ul class="floor-list">
        {{#subList}}
        <li class="floor-item">
            <a href="./list.html?categoryId={{id}}">
                <span class="floor-text">{{name}}</span>
                <img class="floor-img" src="{{imageHost}}{{image}}" alt="{{name}}" />
            </a>
        </li>
        {{/subList}}
    </ul>
{{/list}}